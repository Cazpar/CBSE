<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="player" class="org.springlab.playersystem.Player"/>
    <bean id="playerPlugin" class="org.springlab.playersystem.PlayerPlugin"/>
    <bean id="playerControlSystem" class="org.springlab.playersystem.PlayerControlSystem"/>

    <bean id="enemy" class="org.springlab.enemysystem.Enemy"/>
    <bean id="enemyPlugin" class="org.springlab.enemysystem.EnemyPlugin"/>
    <bean id="enemyControlSystem" class="org.springlab.enemysystem.EnemyControlSystem"/>
</beans>